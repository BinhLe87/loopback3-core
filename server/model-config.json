{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db"
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "library": {
    "dataSource": "cc_mysql",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "create": true,
          "find": true
        }
      }
    }
  },
  "program": {
    "dataSource": "cc_mysql",
    "public": true
  },
  "workbook": {
    "dataSource": "cc_mysql",
    "public": true
  }
}
