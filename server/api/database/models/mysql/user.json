{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "ttl": -1,
  "options": {
    "allowEternalTokens": true
  },
  "properties": {
    "name": {
      "type": "string"      
    },
    "role": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "settings": {
      "type": "hasMany",
      "model": "user_setting",
      "foreignKey": "userId"
    },
    "notes": {
      "type": "hasMany",
      "model": "user_note",
      "foreignKey": "userId"
    },
    "workbook_shares": {
      "type": "hasMany",
      "model": "workbook",
      "foreignKey": "userId",
      "through": "workbook_share"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
  
}
