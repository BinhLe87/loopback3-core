{
  "name": "user_program",
  "description": [
    "contains list of program that user has signed-up"
  ],
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "lib_program_user_idx": {
      "keys": {
        "programId": 1,
        "userId": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "BelongsToIntegrityCheck": false
  },
  "properties": {
  },
  "validations": [],
  "relations": {
    "program": {
      "type": "belongsTo",
      "model": "program",
      "foreignKey": "programId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
