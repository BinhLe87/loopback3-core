{
  "name": "item_attribute_template",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "BelongsToIntegrityCheck": false
  },
  "properties": {
    "display_index": {
      "type": "number",
      "default": 0,
      "description": ["the display order index of the attribute in a item"]
    }
  },
  "validations": [],
  "relations": {
    "itemtype": {
      "type": "belongsTo",
      "model": "item_type",
      "foreignKey": "item_typeId"
    },
    "attribute": {
      "type": "belongsTo",
      "model": "attribute"
    }
  },
  "acls": [],
  "methods": {}
}
