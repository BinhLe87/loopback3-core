{
  "name": "page",
  "plural": "pages",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "ActiveStatusDb": true,
    "BelongsToIntegrityCheck": false
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    }
  },
  "scope": {

    "order": "createdAt DESC"
  },
  "validations": [],
  "relations": {
    "chapters": {
      "type": "hasMany",
      "model": "chapter",
      "foreignKey": "pageId",
      "through": "chapter_page"
    },
    "items": {
      "type": "hasMany",
      "model": "item",
      "foreignKey": "pageId",
      "through": "page_item"
    },
    "page_items": {
      "type": "hasMany",
      "model": "page_item",
      "foreignKey": "pageId"
    },
    "user_notes": {
      "type": "hasMany",
      "model": "user_note",
      "polymorphic": {
        "selector": "noteable"
      }
    }
  },
  "acls": [],
  "methods": {}
}
