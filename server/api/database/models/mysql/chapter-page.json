{
  "name": "chapter_page",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "page_idx": {
      "keys": {
        "pageId": 1
      },
      "options": {
        "unique": true
      }
    },
    "unique_display_in_chapter": {
      "keys": {
        "chapterId": 1,
        "display_index": 1
      },
      "options": {
        "unique": false
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "BelongsToIntegrityCheck": false,
    "SortDisplayIndex": true,
    "AddRelationModel": true
  },
  "properties": {
    "display_index": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "chapter": {
      "type": "belongsTo",
      "model": "chapter",
      "foreignKey": "chapterId"
    },
    "page": {
      "type": "belongsTo",
      "model": "page",
      "foreignKey": "pageId"
    }
  },
  "acls": [],
  "methods": {}
}
