{
  "name": "program",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "BelongsToIntegrityCheck": false
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "libraries": {
      "type": "hasMany",
      "model": "library",
      "foreignKey": "programId",
      "through": "library_program"
    },
    "workbooks": {
      "type": "hasMany",
      "model": "workbook",
      "foreignKey": "programId",
      "through": "program_workbook"
    }
  },
  "acls": [],
  "methods": {}
}
