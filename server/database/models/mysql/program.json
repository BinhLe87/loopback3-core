{
  "name": "program",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "BelongsToIntegrityCheck": false
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "libraries": {
      "type": "hasMany",
      "model": "library",
      "foreignKey": "programId",
      "through": "library_program"
    },
    "workbooks": {
      "type": "hasMany",
      "model": "library_workbook",
      "polymorphic": {
        "foreignKey": "workbook_owner_id",
        "discriminator": "workbook_owner_type"
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
