{
  "name": "item",
  "plural": "items",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "name_idx": {
      "keys": {
        "name": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "BelongsToIntegrityCheck": false
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "content": {
      "type": "object",
      "description": ["contains all attributes' values description"]
    }
  },
  "validations": [],
  "relations": {
    "pages": {
      "type": "hasMany",
      "model": "page",
      "foreignKey": "itemId",
      "through": "page_item"
    },
    "attributes": {
      "type": "hasMany",
      "model": "attribute",
      "foreignKey": "itemId",
      "through": "item_attribute_template"
    },
    "itemtype": {
      "type": "belongsTo",
      "model": "item_type",
      "foreignKey": "item_typeId"
    }
  },
  "acls": [],
  "methods": {}
}
