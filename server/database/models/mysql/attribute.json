{
  "name": "attribute",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "attr_type": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "default": 1
    }
  },
  "indexes": {
    "name_type_idx": {
      "keys": {
        "name": 1,
        "attr_type": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "validations": [],
  "relations": {
    "items": {
      "type": "hasMany",
      "model": "item",
      "foreignKey": "attributeId",
      "through": "item_attribute_value"
    }
  },
  "acls": [],
  "methods": {}
}