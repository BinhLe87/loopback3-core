{
  "name": "chapter",
  "plural": "chapters",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true,
    "BelongsToIntegrityCheck": false
  },
  "properties": {
    "name": {
      "type": "string",
      "index": {
        "unique": true
      }
    },
    "is_visible": {
      "type": "boolean",
      "default": 1
    }
  },
  "validations": [],
  "relations": {
    "workbooks": {
      "type": "hasMany",
      "model": "workbook",
      "foreignKey": "chapterId",
      "through": "workbook_chapter"
    },
    "pages": {
      "type": "hasMany",
      "model": "page",
      "foreignKey": "chapterId",
      "through": "chapter_page"
    }
  },
  "acls": [],
  "methods": {}
}
